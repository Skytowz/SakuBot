    "discord.js": "^14.8.0",
    "dotenv": "^10.0.0",
    "ffmpeg-static": "^5.1.0",
    "fs": "^0.0.1-security",
    "got": "^11.8.3",
    "jsdom": "^21.1.1",
    "libsodium-wrappers": "^0.7.10",
    "seedrandom": "^3.0.5",
